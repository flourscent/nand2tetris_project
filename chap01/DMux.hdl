/**
 * Dmultiplexor.  
 * {a,b} = {in,0} if sel == 0
 *         {0,in} if sel == 1
 */


CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
		Nand(a=sel, b=sel, out=nand1out);
		Nand(a=in, b=sel, out=nand2out);
		Nand(a=in, b=nand1out, out=a);
		Nand(a=nand2out, b=nand2out, out=b);
}
